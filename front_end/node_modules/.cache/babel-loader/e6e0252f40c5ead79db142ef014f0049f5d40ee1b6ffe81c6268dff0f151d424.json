 },\n    async month() {\n      this.socket.open();\n      const res = await axios.get('http://8.134.178.190:5000/user/get_qr_code', {\n        params: {\n          user_id: this.user_id,\n          type: '1'\n        },\n        responseType: 'blob'\n      });\n      if (res.data.code === '400') {\n        alert('获取二维码失败');\n        return;\n      }\n      const imgUrl = URL.createObjectURL(res.data);\n      this.codePicture = imgUrl;\n      this.isShowCode = true;\n      // this.connectWebSocket()\n    },\n\n    async quarter() {\n      this.socket.open();\n      const res = await axios.get('http://8.134.178.190:5000/user/get_qr_code', {\n        params: {\n          user_id: this.user_id,\n          type: '2'\n        },\n        responseType: 'blob'\n      });\n      if (res.data.code === '400') {\n        alert('获取二维码失败');\n        return;\n      }\n      const imgUrl = URL.createObjectURL(res.data);\n      this.codePicture = imgUrl;\n      this.isShowCode = true;\n      // this.connectWebSocket()\n    },\n\n    async year() {\n      this.socket.open();\n      const res = await axios.get('http://8.134.178.190:5000/user/get_qr_code', {\n        params: {\n          user_id: this.user_id,\n          type: '3'\n        },\n        responseType: 'blob'\n      });\n      if (res.data.code === '400') {\n        alert('获取二维码失败');\n        return;\n      }\n      const imgUrl = URL.createObjectURL(res.data);\n      this.codePicture = imgUrl;\n      this.isShowCode = true;\n      // this.connectWebSocket()\n    },\n\n    cancel() {\n      this.isShowCode = false;\n    },\n    changePsd() {\n      this.$router.push({\n        path: '/changePassword',\n        query: {\n          user_id: this.user_id\n        }\n      });\n    },\n    setting() {\n      this.$router.push({\n        path: '/setting',\n        query: {\n          user_id: this.user_id\n        }\n      });\n    },\n    toChat() {\n      this.$router.push({\n        path: '/chatInterface',\n        query: {