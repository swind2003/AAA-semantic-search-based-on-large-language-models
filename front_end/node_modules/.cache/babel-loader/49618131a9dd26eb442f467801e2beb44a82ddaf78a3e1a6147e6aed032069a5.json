{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'SettingInfo',\n  data() {\n    return {\n      noModify: true,\n      user_id: this.$route.query.user_id,\n      mail_account: '',\n      nickname: '',\n      sex: '',\n      file_show1: null,\n      // 初始展示\n      file_show2: null,\n      // 编辑展示\n      file_get: null,\n      // 上传的图片\n      vipDays: 30,\n      //剩余VIP天数\n      isVip: false,\n      // true为VIP用户\n      isClose: false,\n      // true为自动关闭广告开启\n      isCloseShow: false // 展示时确认关闭广告是否开启\n    };\n  },\n\n  created() {\n    document.title = '设置-chataaa';\n    this.getInfo();\n  },\n  methods: {\n    async getInfo() {\n      const res1 = await axios.get('http://119.39.26.149:5000/user/information/get', {\n        params: {\n          user_id: this.user_id\n        }\n      });\n      if (res1.data.code === '400') {\n        alert('操作失败');\n        return;\n      }\n      this.mail_account = res1.data.mail_account;\n      this.nickname = res1.data.nickname, this.sex = res1.data.sex;\n      this.vipDays = res1.data.vip_time;\n      this.isCloseShow = res1.data.advertise_state;\n      if (this.vipDays > 0) {\n        this.isVip = true;\n      }\n      const res2 = await axios.get('http://119.39.26.149:5000/user/information/get_head_portrait', {\n        params: {\n          user_id: this.user_id\n        },\n        responseType: 'blob'\n      });\n      if (res2.data.code === '400') {\n        alert('操作失败');\n        return;\n      }\n      const imgUrl = URL.createObjectURL(res2.data);\n      this.file_show1 = imgUrl;\n      const res3 = await axios.get('http://119.39.26.149:5000/user/get_vip_state', {\n        params: {\n          user_id: this.user_id\n        }\n      });\n      if (res3.data.code === '400') {\n        alert('获取vip身份失败');\n        return;\n      }\n      this.isVip = res3.data.state;\n    },\n    triggerFileInput() {\n      this.$refs.fileInput.click();\n    },\n    handleFileChange(event) {\n      this.file_get = event.target.files[0];\n      this.file_show2 = URL.createObjectURL(this.file_get);\n    },\n    toChat() {\n      this.$router.push({\n        path: '/chatInterface',\n        query: {\n          user_id: this.user_id\n        }\n      });\n    },\n    manageRoles() {\n      if (this.isVip) {\n        this.$router.push({\n          path: '/ManageRoles',\n          query: {\n            user_id: this.user_id\n          }\n        });\n      } else {\n        alert('该功能为VIP用户专享，快去升级会员吧！');\n      }\n    },\n    createRoles() {\n      if (this.isVip) {\n        this.$router.push({\n          path: '/createRoles',\n          query: {\n            user_id: this.user_id\n          }\n        });\n      } else {\n        alert('该功能为VIP用户专享，快去升级会员吧！');\n      }\n    },\n    toFeedback() {\n      this.$router.push({\n        path: '/feedback',\n        query: {\n          user_id: this.user_id\n        }\n      });\n    },\n    recharge() {\n      this.$router.push({\n        path: '/recharge',\n        query: {\n          user_id: this.user_id\n        }\n      });\n    },\n    async exit() {\n      const res = await axios.post('http://119.39.26.149:5000/login_system/log_out', {\n        user_id: this.user_id\n      });\n      if (res.data.code === '400') {\n        alert('退出登录失败');\n        return;\n      }\n      this.$router.push('/login_system');\n    },\n    edit() {\n      this.noModify = false;\n      this.file_show2 = this.file_show1;\n      this.isClose = this.isCloseShow;\n      // console.log(this.file_get)\n    },\n\n    noVip() {\n      alert('“自动关闭广告”功能仅VIP用户可用');\n    },\n    exitEdit() {\n      this.noModify = true;\n      this.file_show2 = this.file_show1;\n      this.isClose = this.isCloseShow;\n    },\n    async chessEdit() {\n      this.noModify = true;\n      const res1 = await axios.post('http://119.39.26.149:5000/user/information/set', {\n        user_id: this.user_id,\n        nickname: this.nickname,\n        sex: this.sex,\n        advertise_state: this.isClose\n      });\n      if (res1.data.code === '400') {\n        alert('操作失败');\n        return;\n      }\n      if (this.file_get !== null) {\n        let file = new FormData();\n        file.append('file', this.file_get);\n        file.append('user_id', this.user_id);\n        const res2 = await axios.post('http://119.39.26.149:5000/user/information/change_headportrait', file\n        // {\n        //   headers: {\n        //     'Content-Type': 'multipart/form-data',\n        //   },\n        // }\n        );\n\n        if (res2.data.code === '400') {\n          alert('头像上传失败');\n          return;\n        }\n        this.file_show1 = this.file_show2;\n      }\n      this.isCloseShow = this.isClose;\n    },\n    changePsd() {\n      this.$router.push({\n        path: '/changePassword',\n        query: {\n          user_id: this.user_id\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","noModify","user_id","$route","query","mail_account","nickname","sex","file_show1","file_show2","file_get","vipDays","isVip","isClose","isCloseShow","created","document","title","getInfo","methods","res1","get","params","code","alert","vip_time","advertise_state","res2","responseType","imgUrl","URL","createObjectURL","res3","state","triggerFileInput","$refs","fileInput","click","handleFileChange","event","target","files","toChat","$router","push","path","manageRoles","createRoles","toFeedback","recharge","exit","res","post","edit","noVip","exitEdit","chessEdit","file","FormData","append","changePsd"],"sources":["src/views/Setting.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapped\">\r\n    <div class=\"left\">\r\n      <ul class=\"leftText\">\r\n        <li @click=\"toChat()\" class=\"one\">聊天框</li>\r\n        <li @click=\"manageRoles()\" class=\"two\">角色管理</li>\r\n        <li @click=\"createRoles()\" class=\"three\">新建角色</li>\r\n        <li class=\"four\">设置</li>\r\n        <li class=\"five\" @click=\"exit()\">退出登录</li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"middle\">\r\n      <div class=\"middleSetting\">设置</div>\r\n      <div class=\"settingTop active\">个人信息</div>\r\n      <div @click=\"changePsd()\" class=\"settingTop\">修改密码</div>\r\n      <div @click=\"toFeedback()\" class=\"settingTop\">提交反馈</div>\r\n      <!-- <div class=\"back\">返回</div> -->\r\n    </div>\r\n    <div class=\"right\">\r\n      <div v-if=\"noModify\" class=\"display\">\r\n          <img :src=\"file_show1\" alt=\"\">\r\n          <!-- <div>头像</div> -->\r\n          <br>\r\n          <span class=\"email\">邮箱： {{ mail_account }}</span>\r\n          <span class=\"nickname\">昵称： {{ nickname }}</span>\r\n          <br>\r\n          <span class=\"sex\">性别： {{ sex }}</span>\r\n          <br>\r\n          <span class=\"vip\">\r\n            <span v-show=\"isVip\">VIP ： 您已经是VIP用户，剩余<strong>{{ vipDays }}</strong>天，继续</span><span @click=\"recharge()\" v-show=\"isVip\" class=\"recharge\">充值</span>\r\n            <span v-show=\"!isVip\">VIP ： 您还不是VIP用户，快去</span><span @click=\"recharge()\" v-show=\"!isVip\" class=\"recharge\">升级会员</span><span v-show=\"!isVip\">吧</span>\r\n          </span>\r\n          <span v-show=\"isCloseShow\" class=\"isClose\">自动关闭广告<strong>已开启</strong></span>\r\n          <span v-show=\"!isCloseShow\" class=\"isClose\">自动关闭广告<strong>未开启</strong></span>\r\n          <div @click=\"edit()\" class=\"edit\">编辑</div>\r\n      </div>\r\n      <div v-else class=\"modifying\">\r\n        <input ref=\"fileInput\" type=\"file\" style=\"display: none\" @change=\"handleFileChange\">\r\n        <div @click=\"triggerFileInput()\">\r\n          <img :src=\"file_show2\" alt=\"\">\r\n        </div>\r\n        <br>\r\n        <span class=\"email\">邮箱： {{ mail_account }}</span>\r\n        <div class=\"nickname\">\r\n          <span>昵称： </span><input v-model=\"nickname\" type=\"text\" placeholder=\"请输入您的昵称\">\r\n        </div>\r\n        <br>\r\n        <div class=\"sex\">\r\n          <span>性别： </span><label for=\"man\"><input v-model=\"sex\" type=\"radio\" name=\"gender\" value=\"男\" id=\"man\"> 男 </label><label for=\"woman\"><input v-model=\"sex\" type=\"radio\" name=\"gender\" value=\"女\" id=\"woman\"> 女</label>\r\n        </div>\r\n        <label v-if=\"isVip\" class=\"closeAdvertisement\"><input v-model=\"isClose\" type=\"checkbox\"> 自动关闭广告</label>\r\n        <label v-else @click=\"noVip()\" class=\"closeAdvertisement\"><input type=\"checkbox\" disabled> 自动关闭广告</label>\r\n        <div @click=\"exitEdit()\" class=\"edit\">退出编辑</div>\r\n        <div @click=\"chessEdit()\" class=\"chess\">确定</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  name: 'SettingInfo',\r\n  data () {\r\n    return {\r\n      noModify: true,\r\n      user_id: this.$route.query.user_id,\r\n      mail_account: '',\r\n      nickname: '',\r\n      sex: '',\r\n      file_show1: null, // 初始展示\r\n      file_show2: null, // 编辑展示\r\n      file_get: null, // 上传的图片\r\n      vipDays: 30, //剩余VIP天数\r\n      isVip: false, // true为VIP用户\r\n      isClose: false, // true为自动关闭广告开启\r\n      isCloseShow: false, // 展示时确认关闭广告是否开启\r\n    }\r\n  },\r\n\r\n  created () {\r\n    document.title = '设置-chataaa'\r\n    this.getInfo()\r\n  },\r\n\r\n  methods: {\r\n    async getInfo () {\r\n      const res1 = await axios.get('http://119.39.26.149:5000/user/information/get', {\r\n        params: {\r\n          user_id: this.user_id\r\n        }\r\n      })\r\n      if (res1.data.code === '400') {\r\n        alert('操作失败')\r\n        return\r\n      }\r\n      this.mail_account = res1.data.mail_account\r\n      this.nickname = res1.data.nickname,\r\n      this.sex = res1.data.sex\r\n      this.vipDays = res1.data.vip_time\r\n      this.isCloseShow = res1.data.advertise_state\r\n      if (this.vipDays > 0) {\r\n        this.isVip = true\r\n      }\r\n      const res2 = await axios.get('http://119.39.26.149:5000/user/information/get_head_portrait', {\r\n        params: {\r\n          user_id: this.user_id\r\n        },\r\n        responseType: 'blob'\r\n      })\r\n      if (res2.data.code === '400') {\r\n        alert('操作失败')\r\n        return\r\n      }\r\n      const imgUrl = URL.createObjectURL(res2.data)\r\n      this.file_show1 = imgUrl\r\n\r\n      const res3 = await axios.get('http://119.39.26.149:5000/user/get_vip_state', {\r\n        params: {\r\n          user_id: this.user_id\r\n        }\r\n      })\r\n      if (res3.data.code === '400') {\r\n        alert('获取vip身份失败')\r\n        return\r\n      }\r\n      this.isVip = res3.data.state\r\n    },\r\n    triggerFileInput () {\r\n      this.$refs.fileInput.click()\r\n    },\r\n    handleFileChange (event) {\r\n      this.file_get = event.target.files[0]\r\n      this.file_show2 = URL.createObjectURL(this.file_get)\r\n    },\r\n    toChat () {\r\n      this.$router.push({\r\n        path: '/chatInterface',\r\n        query: {\r\n          user_id: this.user_id\r\n        }\r\n      })\r\n    },\r\n    manageRoles() {\r\n      if(this.isVip) {\r\n        this.$router.push({\r\n          path: '/ManageRoles',\r\n          query: {\r\n            user_id: this.user_id\r\n          }\r\n        })\r\n      } else {\r\n        alert('该功能为VIP用户专享，快去升级会员吧！')\r\n      }\r\n    },\r\n    createRoles () {\r\n      if (this.isVip) {\r\n        this.$router.push({\r\n          path: '/createRoles',\r\n          query: {\r\n            user_id: this.user_id\r\n          }\r\n        })\r\n      } else {\r\n        alert('该功能为VIP用户专享，快去升级会员吧！')\r\n      }\r\n    },\r\n    toFeedback () {\r\n      this.$router.push({\r\n        path: '/feedback',\r\n        query: {\r\n          user_id: this.user_id\r\n        }\r\n      })\r\n    },\r\n    recharge () {\r\n      this.$router.push({\r\n        path: '/recharge',\r\n        query: {\r\n          user_id: this.user_id\r\n        }\r\n      })\r\n    },\r\n    async exit () {\r\n      const res = await axios.post('http://119.39.26.149:5000/login_system/log_out', {\r\n        user_id: this.user_id\r\n      })\r\n      if (res.data.code === '400') {\r\n        alert('退出登录失败')\r\n        return\r\n      }\r\n      this.$router.push('/login_system')\r\n    },\r\n    edit () {\r\n      this.noModify = false\r\n      this.file_show2 = this.file_show1\r\n      this.isClose = this.isCloseShow\r\n      // console.log(this.file_get)\r\n    },\r\n    noVip () {\r\n      alert('“自动关闭广告”功能仅VIP用户可用')\r\n    },\r\n    exitEdit () {\r\n      this.noModify = true\r\n      this.file_show2 = this.file_show1\r\n      this.isClose = this.isCloseShow\r\n    },\r\n    async chessEdit () {\r\n      this.noModify = true\r\n      const res1 = await axios.post('http://119.39.26.149:5000/user/information/set', {\r\n        user_id: this.user_id,\r\n        nickname: this.nickname,\r\n        sex: this.sex,\r\n        advertise_state: this.isClose\r\n      })\r\n      if (res1.data.code === '400') {\r\n        alert('操作失败')\r\n        return\r\n      }\r\n      if (this.file_get !== null) {\r\n        let file = new FormData()\r\n        file.append('file', this.file_get)\r\n        file.append('user_id', this.user_id)\r\n        const res2 = await axios.post('http://119.39.26.149:5000/user/information/change_headportrait', file\r\n        // {\r\n        //   headers: {\r\n        //     'Content-Type': 'multipart/form-data',\r\n        //   },\r\n        // }\r\n        )\r\n        if (res2.data.code === '400') {\r\n          alert('头像上传失败')\r\n          return\r\n        }\r\n        this.file_show1 = this.file_show2\r\n      }\r\n      this.isCloseShow = this.isClose\r\n    },\r\n    changePsd () {\r\n      this.$router.push({\r\n        path: '/changePassword',\r\n        query: {\r\