{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ManageUsers',\n  data() {\n    return {\n      user_id: '',\n      // loginShow: true, // 显示限制登录/允许登录\n      showLoginLimit: false,\n      // 是否显示选择限制登录天数窗口\n      // timesShow: true, // 显示限制次数/解除限次\n      showTimesLimit: false,\n      // 是否显示选择限制次数窗口\n      user_information: [],\n      // 存储用户信息\n      index: 0,\n      limitDays: '1',\n      // 获取限制登录的天数\n      limittimes: '1',\n      // 获取限制的次数\n      mail_account: '',\n      // 搜索输入的邮箱\n      nickname: '',\n      // 搜索输入的昵称\n      pictures: [] // 头像\n    };\n  },\n\n  created() {\n    this.getInfo();\n  },\n  // mounted () {\n  //   this.getPictures(this.user_id)\n  // },\n\n  methods: {\n    setInfo() {\n      this.$router.push('/setInformation');\n    },\n    seeInfo() {\n      this.$router.push('/seeInformation');\n    },\n    exit() {\n      this.$router.push('/login_system');\n    },\n    async getInfo() {\n      const res = await axios.get('http://119.39.26.104:5000/administrator/get_all_user');\n      this.user_information = res.data.data;\n      // console.log(res)\n      this.getPictures(this.user_information);\n    },\n    async getPictures(user_information) {\n      let length = user_information.length();\n      for (let i = 0; i < length; i++) {\n        const res = await axios.get('http://119.39.26.104:5000/user/information/get_head_portrait', {\n          params: {\n            user_id: user_information[i].user_id\n          },\n          responseType: 'blob'\n        });\n        // console.log(res)\n        const imgUrl = URL.createObjectURL(res.data);\n        this.pictures.push(imgUrl);\n      }\n    },\n    async search() {\n      // 查询\n      const res = await axios.get('http://119.39.26.104:5000/administrator/search_user', {\n        params: {\n          mail_account: this.mail_account,\n          nickname: this.nickname\n        }\n      });\n      console.log(res);\n      this.user_information = res.data.data;\n      this.mail_account = '';\n      this.nickname = '';\n      this.getPictures(this.user_information);\n    },\n    limitLogin(index, user_id) {\n      // 点击限制次数登录\n      this.showLoginLimit = true;\n      this.index = index;\n      this.user_id = user_id;\n    },\n    async yesLogin() {\n      // 确定限制登录\n      this.showLoginLimit = false;\n      this.user_information;\n      this.user_information[this.index].if_login_limit = 'true';\n      const res = await axios.post('http://119.39.26.104:5000/administrator/restrict_login', {\n        user_id: this.user_id,\n        number_time: this.limitDays\n      });\n      console.log(res);\n      // this.loginShow = false\n      // this.getInfo()\n    },\n\n    noLogin() {\n      // 取消限制登录选择\n      this.showLoginLimit = false;\n    },\n    async allowLogin(index, user_id) {\n      // 解除登录限制\n      this.loginShow = true;\n      this.index = index;\n      this.user_id = user_id;\n      this.user_information[this.index].if_login_limit = 'false';\n      const res = await axios.post('http://119.39.26.104:5000/administrator/restrict_login', {\n        user_id: this.user_id,\n        number_time: '0'\n      });\n      console.log(res);\n      // this.getInfo()\n    },\n\n    limitTimes(index, user_id) {\n      // 点击限制次数\n      this.showTimesLimit = true;\n      this.index = index;\n      this.user_id = user_id;\n    },\n    async yesTimes() {\n      // 确定限制次数\n      this.showTimesLimit = false;\n      this.user_information[this.index].if_times_limit = 'true';\n      const res = await axios.post('http://119.39.26.104:5000/administrator/restrict_query', {\n        user_id: this.user_id,\n        query_times: this.limittimes\n      });\n      console.log(res);\n      // this.timesShow = false\n      // this.getInfo()\n    },\n\n    noTimes() {\n      // 取消限制次数选择\n      this.showTimesLimit = false;\n    },\n    async allowTimes(index, user_id) {\n      // 解除次数限制\n      // this.timesShow = true\n      this.index = index;\n      this.user_id = user_id;\n      this.user_information[this.index].if_times_limit = 'false';\n      const res = await axios.post('http://119.39.26.104:5000/administrator/restrict_query', {\n        user_id: this.user_id,\n        query_times: '8'\n      });\n      console.log(res);\n      // this.getInfo()\n    },\n\n    async del(user_id) {\n      const res = await axios.delete(`http://119.39.26.104:5000/administrator/delete_user?user_id=${user_id}`);\n      console.log(res);\n      this.getInfo();\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","user_id","showLoginLimit","showTimesLimit","user_information","index","limitDays","limittimes","mail_account","nickname","pictures","created","getInfo","methods","setInfo","$router","push","seeInfo","exit","res","get","getPictures","length","i","params","responseType","imgUrl","URL","createObjectURL","search","console","log","limitLogin","yesLogin","if_login_limit","post","number_time","noLogin","allowLogin","loginShow","limitTimes","yesTimes","if_times_limit","query_times","noTimes","allowTimes","del","delete"],"sources":["src/views/ManageUsers.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapped\">\r\n    <div class=\"left\">\r\n      <ul class=\"leftText\">\r\n        <li @click=\"setInfo()\" class=\"one\">信息设置</li>\r\n        <li class=\"two\">用户管理</li>\r\n        <li @click=\"seeInfo()\" class=\"three\">查看信息</li>\r\n        <li class=\"four\">用户反馈</li>\r\n        <li class=\"five\" @click=\"exit()\">退出登录</li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"right\">\r\n      <h2>用户管理</h2>\r\n      <div class=\"inp1\">\r\n      <span>用户邮箱： </span><input v-model=\"mail_account\" type=\"text\" placeholder=\"请输入用户邮箱\">\r\n      </div>\r\n      <div class=\"inp2\">\r\n        <span>用户昵称： </span><input v-model=\"nickname\" type=\"text\" placeholder=\"请输入用户昵称\">\r\n      </div>\r\n      <button @click=\"search()\" class=\"query\">查询</button>\r\n      <div class=\"Table\">\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th style=\"width: 100px;\">头像</th>\r\n              <th style=\"width: 300px;\">用户邮箱</th>\r\n              <th style=\"width: 250px;\">用户昵称</th>\r\n              <th style=\"width: 100px;\">性别</th>\r\n              <th style=\"width: 100px;\">VIP</th>\r\n              <th style=\"width: 400px;\">操作</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(info, index1) in user_information\" :key=\"index1\">\r\n              <td v-for=\"(pciture, index2) in pictures\" :key=\"index2\">\r\n                <img :src=\"picture[index2]\" alt=\"\">\r\n              </td>\r\n              <td>{{ info.mail_account }}</td>\r\n              <td>{{ info.nickname }}</td>\r\n              <td>{{ info.sex }}</td>\r\n              <td>否</td>\r\n              <td>\r\n                <button v-if=\"info.if_login_limit === 'false'\" class=\"operation\" @click=\"limitLogin(index1, info.user_id)\">限制登录</button>\r\n                <button v-else-if=\"info.if_login_limit === 'true'\" class=\"operation active\" @click=\"allowLogin(index1, info.user_id)\">允许登录</button>\r\n                <button v-if=\"info.if_times_limit === 'false'\" class=\"operation\" @click=\"limitTimes(index1, info.user_id)\">限制次数</button>\r\n                <button v-else-if=\"info.if_times_limit === 'true'\" class=\"operation active\" @click=\"allowTimes(index1, info.user_id)\">解除限次</button>\r\n                <button @click=\"del(info.user_id)\" class=\"operation\" >删除用户</button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div class=\"limitWindow\" v-show=\"showLoginLimit\">\r\n        <h3>限制登录</h3>\r\n        <span>限制时间：</span>\r\n        <select v-model=\"limitDays\" name=\"\" id=\"\">\r\n          <option value=\"1\">1天</option>\r\n          <option value=\"3\">3天</option>\r\n          <option value=\"5\">5天</option>\r\n          <option value=\"7\">7天</option>\r\n          <option value=\"14\">14天</option>\r\n        </select>\r\n        <br>\r\n        <button @click=\"yesLogin()\">确定</button> <button @click=\"noLogin()\">取消</button>\r\n      </div>\r\n      <div class=\"limitWindow\" v-show=\"showTimesLimit\">\r\n        <h3>限制次数</h3>\r\n        <span>限制每小时次数：</span>\r\n        <select v-model=\"limittimes\" name=\"\" id=\"\">\r\n          <option value=\"1\">1次/h</option>\r\n          <option value=\"3\">3次/h</option>\r\n          <option value=\"5\">5次/h</option>\r\n        </select>\r\n        <br>\r\n        <button @click=\"yesTimes()\">确定</button> <button @click=\"noTimes()\">取消</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  name: 'ManageUsers',\r\n  data () {\r\n    return {\r\n      user_id: '',\r\n      // loginShow: true, // 显示限制登录/允许登录\r\n      showLoginLimit: false, // 是否显示选择限制登录天数窗口\r\n      // timesShow: true, // 显示限制次数/解除限次\r\n      showTimesLimit: false, // 是否显示选择限制次数窗口\r\n      user_information: [], // 存储用户信息\r\n      index: 0,\r\n      limitDays: '1', // 获取限制登录的天数\r\n      limittimes: '1', // 获取限制的次数\r\n      mail_account: '', // 搜索输入的邮箱\r\n      nickname: '', // 搜索输入的昵称\r\n      pictures: [], // 头像\r\n    }\r\n  },\r\n\r\n  created () {\r\n    this.getInfo()\r\n  },\r\n\r\n  // mounted () {\r\n  //   this.getPictures(this.user_id)\r\n  // },\r\n\r\n  methods: {\r\n    setInfo () {\r\n      this.$router.push('/setInformation')\r\n    },\r\n    seeInfo () {\r\n      this.$router.push('/seeInformation')\r\n    },\r\n    exit () {\r\n      this.$router.push('/login_system')\r\n    },\r\n    async getInfo () {\r\n      const res = await axios.get('http://119.39.26.104:5000/administrator/get_all_user')\r\n      this.user_information = res.data.data\r\n      // console.log(res)\r\n      this.getPictures(this.user_information)\r\n    },\r\n    async getPictures (user_information) {\r\n      let length = user_information.length()\r\n      for (let i = 0 ; i < length; i++) {\r\n        const res = await axios.get('http://119.39.26.104:5000/user/information/get_head_portrait', {\r\n          params: {\r\n            user_id: user_information[i].user_id\r\n          },\r\n          responseType: 'blob'\r\n        })\r\n        // console.log(res)\r\n        const imgUrl = URL.createObjectURL(res.data)\r\n        this.pictures.push(imgUrl)\r\n      }\r\n    },\r\n    async search () { // 查询\r\n      const res = await axios.get('http://119.39.26.104:5000/administrator/search_user', {\r\n        params: {\r\n          mail_account: this.mail_account,\r\n          nickname: this.nickname\r\n        }\r\n      })\r\n      console.log(res)\r\n      this.user_information = res.data.data\r\n      this.mail_account = ''\r\n      this.nickname = ''\r\n      this.getPictures(this.user_information)\r\n    },\r\n    limitLogin (index, user_id) { // 点击限制次数登录\r\n      this.showLoginLimit = true\r\n      this.index = index\r\n      this.user_id = user_id\r\n    },\r\n    async yesLogin () { // 确定限制登录\r\n      this.showLoginLimit = false\r\n      this.user_information\r\n      this.user_information[this.index].if_login_limit = 'true'\r\n      const res = await axios.post('http://119.39.26.104:5000/administrator/restrict_login', {\r\n        user_id: this.user_id,\r\n        number_time: this.limitDays,\r\n      })\r\n      console.log(res)\r\n      // this.loginShow = false\r\n      // this.getInfo()\r\n    },\r\n    noLogin () { // 取消限制登录选择\r\n      this.showLoginLimit = false\r\n    },\r\n    async allowLogin (index, user_id) { // 解除登录限制\r\n      this.loginShow = true\r\n      this.index = index\r\n      this.user_id = user_id\r\n      this.user_information[this.index].if_login_limit = 'false'\r\n      const res = await axios.post('http://119.39.26.104:5000/administrator/restrict_login', {\r\n        user_id: this.user_id,\r\n        number_time: '0',\r\n      })\r\n      console.log(res)\r\n      // this.getInfo()\r\n    },\r\n    limitTimes (index, user_id) { // 点击限制次数\r\n      this.showTimesLimit = true\r\n      this.index = index\r\n      this.user_id = user_id\r\n    },\r\n    async yesTimes () { // 确定限制次数\r\n      this.showTimesLimit = false\r\n      this.user_information[this.index].if_times_limit = 'true'\r\n      const res = await axios.post('http://119.39.26.104:5000/administrator/restrict_query', {\r\n        user_id: this.user_id,\r\n        query_times: this.limittimes,\r\n      })\r\n      console.log(res)\r\n      // this.timesShow = false\r\n      // this.getInfo()\r\n    },\r\n    noTimes () { // 取消限制次数选择\r\n      this.showTimesLimit = false\r\n    },\r\n    async allowTimes (index, user_id) { // 解除次数限制\r\n      // this.timesShow = true\r\n      this.index = index\r\n      this.user_id = user_id\r\n      this.user_information[this.index].if_times_limit = 'false'\r\n      const res = await axios.post('http://119.39.26.104:5000/administrator/restrict_query', {\r\n        user_id: this.user_id,\r\n        query_times: '8',\r\n      })\r\n      console.log(res)\r\n      // this.getInfo()\r\n    },\r\n    async del(user_id) {\r\n      const res = await axios.delete(`http://119.39.26.104:5000/administrator/delete_user?user_id=${user_id}`)\r\n      console.log(res)\r\n      this.getInfo()\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .wrapped {\r\n    display: flex;\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: #465069;\r\n    opacity: 0.9;\r\n  }\r\n\r\n  .left {\r\n    width: 90px;\r\n    height: 100%;\r\n    background-color: #D6ECFF;\r\n    border-radius: 10px 0 0 10px;\r\n  }\r\n\r\n  .leftText {\r\n    position: relative;\r\n    color: #9CB